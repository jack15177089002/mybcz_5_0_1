<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:internal="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:background="?attr/color_common_bg" android:layout_width="match_parent" android:layout_height="match_parent">
    <ImageButton android:id="@+id/walklisten_home" android:background="#00000000" android:paddingLeft="20dp" android:paddingRight="20dp" android:layout_width="wrap_content" android:layout_height="35dp" android:src="?attr/drawable_home"/>
    <RelativeLayout android:layout_gravity="center" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <ImageView android:id="@+id/walklisten_overturn_image" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="29dp" android:src="@drawable/checkplay_bg_normal_default" android:layout_centerVertical="true"/>
        <RelativeLayout android:gravity="center_vertical" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_toRightOf="@+id/walklisten_overturn_image">
            <TextView android:textSize="@dimen/T9" android:textColor="?attr/color_button_text_mediabar_pressed" android:id="@+id/walklisten_overturn_text" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="13dp" android:text="@string/walklisten_overturn"/>
            <TextView android:textSize="@dimen/T4" android:textColor="?attr/color_button_text_mediabar" android:id="@+id/walklisten_overturn_text_sub" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="35dp" android:text="@string/walklisten_overturn_sub" android:layout_below="@+id/walklisten_overturn_text"/>
            <CheckBox android:textSize="@dimen/T2" android:textColor="?attr/color_button_text_mediabar" android:id="@+id/walklisten_check_play_zhanhuwei" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/walklisten_play_zhanhuwei" android:layout_below="@+id/walklisten_overturn_text_sub"/>
        </RelativeLayout>
    </RelativeLayout>
</FrameLayout>
